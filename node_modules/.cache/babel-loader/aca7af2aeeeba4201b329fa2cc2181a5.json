arning\" @click=\"reset\">重置</el-button>\r\n  </div>\r\n\r\n  <div style=\"margin: 10px 0\">\r\n    <el-button type=\"primary\" @click=\"start\">开始 <i class=\"el-icon-video-play\"></i></el-button>\r\n    <el-button class=\"ml-5\" type=\"primary\">导出 <i class=\"el-icon-top\"></i></el-button>\r\n    <el-popconfirm\r\n        cancel-button-text='我再想想'\r\n        class=\"ml-5\"\r\n        confirm-button-text='确定'\r\n        icon=\"el-icon-info\"\r\n        icon-color=\"red\"\r\n        title=\"您确定删除吗？\"\r\n        @confirm=\"delBatch\"\r\n    >\r\n      <el-button slot=\"reference\" type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n    </el-popconfirm>\r\n    >\r\n  </div>\r\n\r\n  <el-table :data=\"tableData\" :header-cell-class-name=\"headerBg\" border stripe\r\n            @selection-change=\"handleSelectionChange\">\r\n    <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n    <el-table-column label=\"设备ID\" prop=\"deviceId\" width=\"200\">\r\n    </el-table-column>\r\n    <el-table-column label=\"传感器ID\" prop=\"sensorId\" width=\"200\">\r\n    </el-table-column>\r\n    <el-table-column label=\"采样值\" prop=\"senValue\" width=\"150\">\r\n    </el-table-column>\r\n    <el-table-column label=\"采样时间\" prop=\"createDate\">\r\n    </el-table-column>\r\n    <el-table-column align=\"center\" label=\"操作\" width=\"200\">\r\n      <template slot-scope=\"scope\">\r\n        <el-button type=\"success\" @click=\"handleEdit(scope.row)\">修改<i class=\"el-icon-edit\"></i></el-button>\r\n        <el-popconfirm\r\n            cancel-button-text='我再想想'\r\n            class=\"ml-5\"\r\n            confirm-button-text='确定'\r\n            icon=\"el-icon-info\"\r\n            icon-color=\"red\"\r\n            title=\"您确定删除吗？\"\r\n            @confirm=\"del(scope.row.id)\"\r\n        >\r\n          <el-button slot=\"reference\" type=\"danger\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n        </el-popconfirm>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <div style=\"padding: 10px 0\">\r\n    <el-pagination\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"[2, 5, 10, 20]\"\r\n        :total=\"total\"\r\n        current-page=\"pageNum\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\">\r\n    </el-pagination>\r\n  </div>\r\n\r\n  <el-dialog :visible.sync=\"dialogFormVisible\" title=\"数据信息\" width=\"30%\">\r\n    <el-form label-width=\"80px\" size=\"small\">\r\n      <el-form-item label=\"设备ID\">\r\n        <el-input v-model=\"form.deviceId\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"传感器ID\">\r\n        <el-input v-model=\"form.sensorId\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"采样值\">\r\n        <el-input v-model=\"form.senValue\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"采样时间\">\r\n        <el-input v-model=\"form.createDate\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Table\",\r\n  tableData: [],\r\n  total: 0,\r\n  pageNum: 1,\r\n  pageSize: 10,\r\n  deviceId: \"\",\r\n  sensorId: \"\",\r\n  createDate: \"\",\r\n  dialogFormVisible: false,\r\n  multipleSelection: [],\r\n  form: {},\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}