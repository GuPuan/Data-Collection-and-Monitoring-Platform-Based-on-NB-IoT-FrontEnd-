ews","sources":["Manage.vue"],"sourcesContent":["<template>\n\n\n  <el-container style=\"min-height: 100vh\">\n      <Aside/>\n    <el-container>\n      <el-header style=\"font-size: 12px; border-bottom: 1px solid #ccc; line-height: 60px; display: flex\">\n        <div style=\"flex: 1; font-size: 20px\">\n          <span :class=\"collapseBtnClass\" style=\"cursor: pointer\" @click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 70px; cursor: pointer\">\n          <span>顾卜铵</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item>个人信息</el-dropdown-item>\n            <el-dropdown-item>退出</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </el-header>\n      <el-main>\n        <div style=\"margin-bottom: 30px\">\n          <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item>数据信息</el-breadcrumb-item>\n          </el-breadcrumb>\n        </div>\n        <div style=\"margin: 10px 0\">\n          <el-input v-model=\"deviceId\" placeholder=\"请输入设备ID\" style=\"width: 200px\"\n                    suffix-icon=\"el-icon-search\"></el-input>\n          <el-input v-model=\"sensorId\" class=\"ml-5\" placeholder=\"请输入传感器ID\" style=\"width: 200px\"\n                    suffix-icon=\"el-icon-search\"></el-input>\n          <el-input v-model=\"createDate\" class=\"ml-5\" placeholder=\"请输入采样时间\" style=\"width: 200px\"\n                    suffix-icon=\"el-icon-time\"></el-input>\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n          <el-button type=\"warning\" @click=\"reset\">重置</el-button>\n        </div>\n\n        <div style=\"margin: 10px 0\">\n          <el-button type=\"primary\" @click=\"start\">开始 <i class=\"el-icon-video-play\"></i></el-button>\n          <el-button class=\"ml-5\" type=\"primary\">导出 <i class=\"el-icon-top\"></i></el-button>\n          <el-popconfirm\n              cancel-button-text='我再想想'\n              class=\"ml-5\"\n              confirm-button-text='确定'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"delBatch\"\n          >\n            <el-button slot=\"reference\" type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n          </el-popconfirm>\n          >\n        </div>\n\n        <el-table :data=\"tableData\" :header-cell-class-name=\"headerBg\" border stripe\n                  @selection-change=\"handleSelectionChange\">\n          <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n          <el-table-column label=\"设备ID\" prop=\"deviceId\" width=\"200\">\n          </el-table-column>\n          <el-table-column label=\"传感器ID\" prop=\"sensorId\" width=\"200\">\n          </el-table-column>\n          <el-table-column label=\"采样值\" prop=\"senValue\" width=\"150\">\n          </el-table-column>\n          <el-table-column label=\"采样时间\" prop=\"createDate\">\n          </el-table-column>\n          <el-table-column align=\"center\" label=\"操作\" width=\"200\">\n            <template slot-scope=\"scope\">\n              <el-button type=\"success\" @click=\"handleEdit(scope.row)\">修改<i class=\"el-icon-edit\"></i></el-button>\n              <el-popconfirm\n                  cancel-button-text='我再想想'\n                  class=\"ml-5\"\n                  confirm-button-text='确定'\n                  icon=\"el-icon-info\"\n                  icon-color=\"red\"\n                  title=\"您确定删除吗？\"\n                  @confirm=\"del(scope.row.id)\"\n              >\n                <el-button slot=\"reference\" type=\"danger\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n              </el-popconfirm>\n            </template>\n          </el-table-column>\n        </el-table>\n        <div style=\"padding: 10px 0\">\n          <el-pagination\n              :page-size=\"pageSize\"\n              :page-sizes=\"[2, 5, 10, 20]\"\n              :total=\"total\"\n              current-page=\"pageNum\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\">\n          </el-pagination>\n        </div>\n\n        <el-dialog :visible.sync=\"dialogFormVisible\" title=\"数据信息\" width=\"30%\">\n          <el-form label-width=\"80px\" size=\"small\">\n            <el-form-item label=\"设备ID\">\n              <el-input v-model=\"form.deviceId\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"传感器ID\">\n              <el-input v-model=\"form.sensorId\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"采样值\">\n              <el-input v-model=\"form.senValue\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"采样时间\">\n              <el-input v-model=\"form.createDate\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </div>\n        </el-dialog>\n\n      </el-main>\n\n    </el-container>\n  </el-container>\n\n</template>\n\n<script>\n// @ is an alias to /src\n\n\n\nimport Aside from \"@/components/Aside\";\n\nexport default {\n  name: 'Home',\n\n  data() {\n    const item = {\n      deviceId: \"\",\n      sensorId: \"\",\n      senValue: \"\",\n      createDate: \"\"\n    };\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      deviceId: \"\",\n      sensorId: \"\",\n      createDate: \"\",\n      msg: \"GuPuan\",\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      headerBg: 'headerBg',\n      dialogFormVisible: false,\n      multipleSelection: [],\n      form: {},\n    }\n  },\n  created() {\n    this.load()\n  },\n\n  components:{\n    Aside\n  },\n\n  methods: {\n    collapse() {  // 点击收缩按钮触发\n      this.isCollapse = !this.isCollapse\n      if (this.isCollapse) {  // 收缩\n        this.sideWidth = 64\n        this.collapseBtnClass = 'el-icon-s-unfold'\n        this.logoTextShow = false\n      } else {   // 展开\n        this.sideWidth = 200\n        this.collapseBtnClass = 'el-icon-s-fold'\n        this.logoTextShow = true\n      }\n    },\n\n    start() {\n      this.request.get(\"/data/start\")\n    },\n\n    save() {\n      this.request.post(\"/data/save\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"修改成功\")\n          this.dialogFormVisible = false\n          this.load()\n        } else {\n          this.$message.error(\"修改失败\")\n        }\n      })\n    },\n\n    del(id) {\n      this.request.delete(\"/data/delete/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"删除失败\")\n        }\n      })\n    },\n\n    handleSelectionChange(val) {\n      this.multipleSelection = val\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id)\n      this.request.post(\"/data/delete/batch\", ids).then(res => {\n        if (res) {\n          this.$message.success(\"批量删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败\")\n        }\n      })\n    },\n\n    load() {\n      this.request.get(\"/data/page\", {\n            params: {\n              pageNum: this.pageNum,\n              pageSize: this.pageSize,\n              deviceId: this.deviceId,\n              sensorId: this.sensorId,\n              createDate: this.createDate,\n\n            }\n          }\n      ).then(res => {\n            console.log(res)\n            this.tableData = res.records\n            this.total = res.total\n          }\n      )\n\n    },\n\n    reset() {\n      this.deviceId = \"\"\n      this.sensorId = \"\"\n      this.createDate = \"\"\n      this.load()\n    },\n\n    handleEdit(row) {\n      this.form = Object.assign({}, row)\n      this.dialogFormVisible = true\n    },\n\n    handleSizeChange(pageSize) {\n      console.log(pageSize)\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum)\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n\n<style>\n.headerBg {\n  background: #eee !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}