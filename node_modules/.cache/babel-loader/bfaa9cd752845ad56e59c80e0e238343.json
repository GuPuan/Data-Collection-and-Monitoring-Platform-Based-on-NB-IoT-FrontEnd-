margin-bottom: 30px\">\r\n    <el-breadcrumb separator=\"/\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">温度</el-breadcrumb-item>\r\n      <el-breadcrumb-item>表格展示</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n  </div>\r\n\r\n  <div style=\"margin: 10px 0\">\r\n    <el-input v-model=\"deviceId\" placeholder=\"请输入设备ID\" style=\"width: 200px\"\r\n              suffix-icon=\"el-icon-search\"></el-input>\r\n    <el-input v-model=\"sensorId\" class=\"ml-5\" placeholder=\"请输入传感器ID\" style=\"width: 200px\"\r\n              suffix-icon=\"el-icon-search\"></el-input>\r\n    <el-input v-model=\"createDate\" class=\"ml-5\" placeholder=\"请输入采样时间\" style=\"width: 200px\"\r\n              suffix-icon=\"el-icon-time\"></el-input>\r\n    <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索<i class=\"el-icon-search\"></i></el-button>\r\n    <el-button type=\"warning\" @click=\"reset\">重置<i class=\"el-icon-refresh-right\"></i></el-button>\r\n    <el-button class=\"ml-5\"  type=\"primary\" @click=\"exportall\">导出 <i class=\"el-icon-top\"></i></el-button>\r\n    <el-popconfirm\r\n        cancel-button-text='我再想想'\r\n        class=\"ml-5\"\r\n        confirm-button-text='确定'\r\n        icon=\"el-icon-info\"\r\n        icon-color=\"red\"\r\n        title=\"您确定删除吗？\"\r\n        @confirm=\"delBatch\">\r\n      <el-button slot=\"reference\" type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n    </el-popconfirm>\r\n  </div>\r\n\r\n  <div style=\"margin: 10px 0\" class=\"ml-5\">\r\n    <el-button-group >\r\n    <el-button type=\"primary\" :plain='currSelect1' size=\"mini\" :disabled='btnDisabled' @click=\"startRec\">开始 <i class=\"el-icon-check\"></i></el-button>\r\n    <el-button type=\"primary\" :plain='currSelect2' size=\"mini\" @click=\"stopRec\">暂停 <i class=\"el-icon-close\"></i></el-button>\r\n    </el-button-group>\r\n    <el-button type=\"primary\" style=\" margin-left: 63px\">预警值</el-button>\r\n    <el-input v-model=\"warnData\" placeholder=\"请设定\" style=\"width: 70px;\"\r\n              ></el-input>\r\n    <el-button type=\"primary\" style=\"margin-left: 66px\">采样周期</el-button>\r\n    <el-select v-model=\"value\" placeholder=\"请选择\" style=\"width: 85px;\">\r\n      <el-option\r\n          v-for=\"item in options\"\r\n          :key=\"item.value\"\r\n          :label=\"item.label\"\r\n          :value=\"item.value\">\r\n      </el-option>\r\n    </el-select>\r\n  </div>\r\n\r\n  <el-table :data=\"tableData\" :header-cell-class-name=\"headerBg\" border stripe\r\n            @selection-change=\"handleSelectionChange\" >\r\n    <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n    <el-table-column align=\"center\" label=\"设备ID\" prop=\"deviceId\" width=\"200\">\r\n    </el-table-column>\r\n    <el-table-column align=\"center\" label=\"传感器ID\" prop=\"sensorId\" width=\"200\">\r\n    </el-table-column>\r\n    <el-table-column align=\"center\" label=\"采样值\" prop=\"senValue\" width=\"150\">\r\n      <template scope=\"scope\">\r\n        <span v-if=\"scope.row.senValue>=warnData\" style=\"color:red\">{{ scope.row.senValue }}</span>\r\n        <span v-else style=\"color: #37B328\">{{ scope.row.senValue }}</span>\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column align=\"center\" label=\"采样时间\" prop=\"createDate\">\r\n    </el-table-column>\r\n    <el-table-column align=\"center\" label=\"操作\" width=\"200\">\r\n      <template slot-scope=\"scope\">\r\n        <el-button type=\"success\" @click=\"handleEdit(scope.row)\">修改<i class=\"el-icon-edit\"></i></el-button>\r\n        <el-popconfirm\r\n            cancel-button-text='我再想想'\r\n            class=\"ml-5\"\r\n            confirm-button-text='确定'\r\n            icon=\"el-icon-info\"\r\n            icon-color=\"red\"\r\n            title=\"您确定删除吗？\"\r\n            @confirm=\"del(scope.row.id)\"\r\n        >\r\n          <el-button slot=\"reference\" type=\"danger\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n        </el-popconfirm>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n  <div style=\"padding: 10px 0\">\r\n    <el-pagination\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"[10,20,50,100]\"\r\n        :total=\"total\"\r\n        current-page=\"pageNum\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\">\r\n    </el-pagination>\r\n  </div>\r\n\r\n  <el-dialog :visible.sync=\"dialogFormVisible\" title=\"数据信息\" width=\"30%\">\r\n    <el-form label-width=\"80px\" size=\"small\">\r\n      <el-form-item label=\"设备ID\">\r\n        <el-input v-model=\"form.deviceId\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"传感器ID\">\r\n        <el-input v-model=\"form.sensorId\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"采样值\">\r\n        <el-input v-model=\"form.senValue\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"采样时间\">\r\n        <el-input v-model=\"form.createDate\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n  <div style=\"width: 1200px;height:580px;\"></div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Table\",\r\n  data() {\r\n    return {\r\n      options: [{\r\n        value: 5,\r\n        label: '5秒'\r\n      }, {\r\n        value: 10,\r\n        label: '10秒'\r\n      }, {\r\n        value: 20,\r\n        label: '20秒'\r\n      }, {\r\n        value: 30,\r\n        label: '30秒',\r\n      }, {\r\n        value: 60,\r\n        label: '60秒'\r\n      }],\r\n      value:\"\",\r\n      btnDisabled:false,\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      deviceId: \"\",\r\n      sensorId: \"\",\r\n      senValue: \"\",\r\n      createDate: \"\",\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      form: {},\r\n      currSelect1:true,\r\n      currSelect2:true,\r\n      warnData:\"\",\r\n      headerBg: 'headerBg',\r\n\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    startRec() {\r\n      this.request.get(\"/data/start/\"+this.value)\r\n    },\r\n\r\n    stopRec() {\r\n      this.request.get(\"/data/stop\")\r\n    },\r\n\r\n    exportall(){\r\n      window.open(\"http://110.40.153.72:9090/data/export\")\r\n    },\r\n\r\n    save() {\r\n      this.request.post(\"/data/save\", this.form).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"修改成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"修改失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    del(id) {\r\n      this.request.delete(\"/data/delete/\" + id).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val\r\n    },\r\n\r\n    delBatch() {\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      this.request.post(\"/data/delete/batch\", ids).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    load() {\r\n      this.request.get(\"/data/page\", {\r\n            params: {\r\n              pageNum: this.pageNum,\r\n              pageSize: this.pageSize,\r\n              deviceId: this.deviceId,\r\n              sensorId: this.sensorId,\r\n              createDate: this.createDate,\r\n            }\r\n          }\r\n      ).then(res => {\r\n            this.tableData = res.records\r\n            this.total = res.total\r\n          }\r\n      )\r\n\r\n    },\r\n\r\n    reset() {\r\n      this.deviceId = \"\"\r\n      this.sensorId = \"\"\r\n      this.createDate = \"\"\r\n      this.load()\r\n    },\r\n\r\n    handleEdit(row) {\r\n      this.form = Object.assign({}, row)\r\n      this.dialogFormVisible = true\r\n    },\r\n\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n\r\n\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style >\r\n.headerBg {\r\n  background: #eee !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}